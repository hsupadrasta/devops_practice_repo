import tkinter as tk
from tkinter import filedialog

def submit():
    file_path = file_path_var.get()
    username = username_var.get()
    password = password_var.get()
    base_url = base_url_var.get()

    print("File Path:", file_path)
    print("Username:", username)
    print("Password:", password)
    print("Base URL:", base_url)

app = tk.Tk()
app.title("Input Form")

# File Path
file_path_label = tk.Label(app, text="File Path:")
file_path_label.pack()

file_path_var = tk.StringVar()
file_path_entry = tk.Entry(app, textvariable=file_path_var)
file_path_entry.pack()

file_path_button = tk.Button(app, text="Browse", command=lambda: file_path_var.set(filedialog.askopenfilename()))
file_path_button.pack()

# Username
username_label = tk.Label(app, text="Username:")
username_label.pack()

username_var = tk.StringVar()
username_entry = tk.Entry(app, textvariable=username_var)
username_entry.pack()

# Password
password_label = tk.Label(app, text="Password:")
password_label.pack()

password_var = tk.StringVar()
password_entry = tk.Entry(app, textvariable=password_var, show="*")
password_entry.pack()

# Base URL
base_url_label = tk.Label(app, text="Base URL:")
base_url_label.pack()

base_url_var = tk.StringVar()
base_url_entry = tk.Entry(app, textvariable=base_url_var)
base_url_entry.pack()

# Submit Button
submit_button = tk.Button(app, text="Submit", command=submit)
submit_button.pack()

app.mainloop()
