import csv
import networkx as nx
import matplotlib.pyplot as plt

# Create an empty graph
G = nx.Graph()

# Read the CSV file and add nodes and edges to the graph
with open('your_csv_file.csv', 'r') as csvfile:
    csvreader = csv.reader(csvfile)
    for row in csvreader:
        entity1 = row[0].strip()
        entity2 = row[2].strip()
        impact = row[1].strip()
        
        # Add nodes and edges to the graph
        G.add_node(entity1)
        G.add_node(entity2)
        G.add_edge(entity1, entity2, impact=impact)

# Visualize the graph (optional)
pos = nx.spring_layout(G)
labels = nx.get_edge_attributes(G, 'impact')
nx.draw(G, pos, with_labels=True, node_size=800, node_color='skyblue', font_size=10)
nx.draw_networkx_edge_labels(G, pos, edge_labels=labels, font_size=8, font_color='red')
plt.show()
